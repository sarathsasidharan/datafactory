{
	"name": "DistribtuionDataStore",
	"properties": {
		"description": "Data Store For distribution",
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorageGen2",
			"type": "LinkedServiceReference"
		},
		"annotations": [
			"datastore"
		],
		"type": "AzureBlobFSFile",
